let handler = async (m, { conn }) => {
    let res = await fetch(API('xteam', '/randomimage/wpmobile', {}, 'APIKEY'))
    if (!res.ok) throw eror
    let img = await res.buffer()
    if (!img) throw img
    conn.sendFile(m.chat, img, '', '¬© sekha', m, 0, { thumbnail: await (await fetch(img)).buffer() })
}
